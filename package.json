{
  "name": "midiateca",
  "version": "0.1.13",
  "description": " Midiateca Capixaba - a child theme of Blocksy",
  "main": "index.js",
  "scripts": {
    "css-minify": "cleancss --level 1 --format breaksWith=lf --source-map --source-map-inline-sources --output style.min.css style.css",
    "css-editor-minify": "cleancss --level 1 --format breaksWith=lf --source-map --source-map-inline-sources --output editor.min.css ./css/block-styles.css",
    "build": "npm run css-minify && npm run css-editor-minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tainacan/midiateca.git"
  },
  "author": "wetah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tainacan/midiateca/issues"
  },
  "homepage": "https://github.com/tainacan/midiateca#readme",
  "devDependencies": {
    "clean-css-cli": "^4.3.0"
  }
}
